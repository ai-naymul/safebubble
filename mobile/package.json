{
  "name": "safebubble-mobile",
  "version": "1.0.0",
  "description": "SafeBubble Mobile App - Solana Token Risk Scanner",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "node scripts/start-with-ip.js",
    "start:ip": "node scripts/start-with-ip.js",
    "start:lan": "expo start --lan",
    "start:tunnel": "expo start --tunnel",
    "start:manual": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "submit:android": "eas submit --platform android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "test": "jest"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.1.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/stack": "^7.4.10",
    "@solana-mobile/mobile-wallet-adapter-protocol": "^2.2.4",
    "@solana-mobile/mobile-wallet-adapter-protocol-web3js": "^2.2.4",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.90.3",
    "axios": "^1.12.2",
    "babel-preset-expo": "^54.0.4",
    "d3-force": "^3.0.0",
    "expo": "^54.0.13",
    "expo-asset": "^12.0.9",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.15",
    "expo-device": "~8.0.9",
    "expo-font": "^14.0.9",
    "expo-haptics": "^15.0.7",
    "expo-linear-gradient": "^15.0.7",
    "expo-notifications": "~0.32.12",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "^15.0.8",
    "nativewind": "4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-quick-base64": "^2.2.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@react-native-community/cli": "^20.0.2",
    "@types/d3-force": "^3.0.9",
    "@types/jest": "^29.5.11",
    "@types/react": "^19.1.0",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "nativewind": {
      "react-native-css-interop": "0.1.22"
    }
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-reanimated",
        "@types/react",
        "react-native-svg"
      ]
    }
